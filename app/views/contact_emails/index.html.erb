<h1>Listing contact_emails</h1>

<table>
  <tr>
    <th>To</th>
    <th>Name</th>
    <th>Address</th>
    <th>Body</th>
    <th>Subject</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contact_emails.each do |contact_email| %>
  <tr>
    <td><%= contact_email.to %></td>
    <td><%= contact_email.name %></td>
    <td><%= contact_email.address %></td>
    <td><%= contact_email.body %></td>
    <td><%= contact_email.subject %></td>
    <td><%= link_to 'Show', contact_email %></td>
    <td><%= link_to 'Edit', edit_contact_email_path(contact_email) %></td>
    <td><%= link_to 'Destroy', contact_email, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact email', new_contact_email_path %>
